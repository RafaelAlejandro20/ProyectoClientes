CREATE TABLE CLIENTES_SOLTE(
ID INT PRIMARY KEY,
NOMBRE_COMPLETO VARCHAR2(100) NOT NULL,
PUESTO VARCHAR2(100) NOT NULL,
SALARIO NUMBER(8,2) NOT NULL,
FECHA_ENTRADA DATE NOT NULL
);

